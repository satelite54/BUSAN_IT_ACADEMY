<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.satelite54.myBoard.model">
	
	<select id="getTime" resultType="java.sql.Date">
		SELECT sysdate FROM dual
	</select>
	
	<select id="getBoardList" resultType="com.satelite54.myBoard.model.board.dto.BoardDTO">
		SELECT BNum,
		BTitle,
		BContent,
		BDate,
		id,
		authority,
		views
	</select>
</mapper>